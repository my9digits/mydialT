  {
  "name": "myapp",
  "description": "a really cool app",
  "version": "1.0.0",
  "engines": {
    "node": "6.11.1"
  }
}
  
  "license": "Apache",
  "private": true,
  "proxy": "http://localhost:4000",
  "scripts": {
    "start:ui": "BROWSER=none node scripts/start.js",
    "start:server": "cd server && npm run start:dev",
    "prestart": "node scripts/prestart.js",
    "start": "concurrently npm:start:server npm:start:ui",
    "start:prod": "cd server && npm run start:prod",
    "start:e2e": "cross-env NODE_ENV=e2e npm run start",
    "start:cypress": "cd e2e && npm run test",
    "postinstall": "cd server && npm install",
    "build": "node scripts/build.js && cd server && npm run build",
    "test:ui": "node scripts/test.js --env=jest-environment-jsdom-sixteen",
    "test:server": "cd server && npm run test",
    "test:e2e": "start-server-and-test start:e2e http://localhost:3000 start:cypress",
    "test:ci": "npm run test:ui -- --watchAll=false --coverage && npm run test:server",
    "lint:ui": "eslint 'client/src' --ext .js,.jsx,.ts,.tsx --no-error-on-unmatched-pattern --color --fix",
    "lint:server": "cd server && npm run lint",
    "lint": "npm run lint:ui && npm run lint:server",
    "format": "prettier --write ./client/src/**/*.{ts,tsx} ./e2e/cypress/integration/*.ts ",
    "format:server": "cd server && npm run format"
  },
  "dependencies": {
    "@babel/core": "7.9.0",
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.50",
    "@svgr/webpack": "4.3.3",
    "@types/socket.io-client": "^1.4.33",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "axios": "^0.19.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "body-parser": "^1.19.0",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "express": "^4.17.1",
    "file-loader": "4.3.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "libphonenumber-js": "^1.7.50",
    "lodash.isempty": "^4.4.0",
    "lodash.upperfirst": "^4.3.1",
    "mini-css-extract-plugin": "0.9.0",
    "moment": "^2.25.3",
    "pnp-webpack-plugin": "1.6.4",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "resolve": "1.15.0",
    "resolve-url-loader": "3.1.1",
    "semver": "6.3.0",
    "socket.io-client": "^2.3.0",
    "start-server-and-test": "^1.11.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "2.3.5",
    "ts-pnp": "1.1.6",
    "twilio": "3.43.0",
    "url-loader": "2.3.0",
    "use-http": "1.0.2",
    "webpack": "4.42.0",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/react-hooks": "^3.3.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/lodash.isempty": "^4.4.6",
    "@types/lodash.upperfirst": "^4.3.6",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.33",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.8",
    "@types/react-router": "^5.1.5",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux": "^3.6.0",
    "concurrently": "^5.2.0",
    "envfile": "^5.1.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-airbnb-typescript": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "husky": "^4.2.5",
    "inquirer": "^7.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-github-reporter": "^0.4.4",
    "jest-html-reporters": "^1.2.1",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.2.2",
    "mockdate": "^3.0.2",
    "moment-timezone": "^0.5.28",
    "ngrok": "^3.2.7",
    "open": "^7.0.4",
    "prettier": "2.0.5",
    "prettier-eslint": "9.0.1",
    "prettier-eslint-cli": "5.0.0",
    "react-test-renderer": "^16.13.1",
    "supertest": "^4.0.2",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "server/**/*.{ts}": [
      "npm run format:server",
      "npm run lint:server",
      "npm run test:server"
    ],
    "client/src/**/*.{ts,tsx}": [
      "prettier --config .prettierrc --write",
      "npm run lint:ui",
      "npm run test -- --bail --watchAll=false --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "globalSetup": "./global-setup.js",
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "client/src/components/**/*.{ts,tsx}",
      "client/src/hooks/**/*.{ts,tsx}",
      "client/src/redux/**/*.{ts,tsx}",
      "!**/index.{ts,tsx}",
      "!**/types.{ts,tsx}",
      "!**/styles.{ts,tsx}",
      "!**/constants.{ts,tsx}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/client/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/client/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/client/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/scripts/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/scripts/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/client/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "jsx"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "testEnvironment": "jest-environment-jsdom-sixteen",
    "coverageDirectory": "../reports/coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  },
  "jest-junit": {
    "suiteName": "client tests",
    "outputDirectory": "./reports/ui-test-results",
    "outputName": "junit-results.xml"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
